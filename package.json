{
  "name": "@v8-heap-inspector/v8-heap-inspector",
  "version": "0.0.4",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "v8-heap-inspector": "bin.js"
  },
  "napi": {
    "name": "v8-heap-inspector",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-pc-windows-msvc",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "AGPL-3.0",
  "devDependencies": {
    "@napi-rs/cli": "^2.16.1",
    "@types/express": "^4.17.17",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js binding.js --dts binding.d.ts && npm run build:ts",
    "build:ts": "tsc",
    "clean": "rimraf -g bin.js binding.js bin.d.ts binding.d.ts v8*.node cli/**/**/*.js",
    "build:debug": "napi build --platform --js binding.js --dts binding.d.ts",
    "prepublishOnly": "npm run build:ts && napi prepublish -t npm",
    "test": "echo 'hello'",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "@types/node": "^20.4.0",
    "arg": "^5.0.2",
    "chalk": "4",
    "express": "^4.18.2"
